{
	"name": "img-preview",
	"displayName": "img preview",
	"description": "a image preview plugin",
	"version": "0.0.1",
	"publisher": "daguang",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:picturePreview"
	],
	"main": "./src/extension",
	"contributes": {
		"commands": [
			{
				"command": "picturePreview",
				"title": "预览文件夹下图片 images preview"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "picturePreview",
					"group": "1_modification",
					"when": "explorerResourceIsFolder"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}